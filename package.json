{
  "name": "reactorx",
  "version": "0.4.2",
  "description": "CSP Reactive flux implementation",
  "author": {
    "name": "Juan B. Rodriguez"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jbrodriguez/reactorx.git"
  },
  "keywords": [
    "flux",
    "react",
    "csp"
  ],
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel src --source-maps inline --presets es2015 --out-dir tmp && regenerator tmp dist",
    "csp": "regenerator node_modules/js-csp/src src/js-csp",
    "clean": "rimraf dist && rimraf tmp",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "js-csp": "^0.5.0",
    "mocha": "^2.3.4",
    "regenerator": "^0.8.42",
    "rimraf": "^2.5.0"
  },
  "bugs": {
    "url": "https://github.com/jbrodriguez/reactorx/issues"
  },
  "homepage": "https://github.com/jbrodriguez/reactorx",
  "maintainers": [
    {
      "name": "jbrodriguez"
    }
  ]
}
